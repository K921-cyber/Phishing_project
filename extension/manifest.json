{
  "manifest_version": 3,
  "name": "Sentinull - Phishing Detector",
  "version": "1.0",
  "description": "Capstone Project: Real-time Phishing Detection",
  "permissions": [
    "activeTab",
    "scripting" 
  ],
  "host_permissions": [
    "https://mail.google.com/*",
    "http://127.0.0.1:5000/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "action": {
    "default_popup": "popup.html"
  }
}